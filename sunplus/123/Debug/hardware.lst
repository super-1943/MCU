                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from port
                            	// 	int SP_Import(unsigned int Port)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_Import: .PROC
000001B7 88 DA              	        PUSH bp TO [sp] 
000001B8 08 0B 01 00        	        bp = sp + 1 
                            	        
000001BA 03 92              			r1 = [bp+3]  						// Port Number
000001BB C1 92              			r1 = [r1]  
                            					
000001BC 88 98              	        POP bp FROM [sp] 
000001BD 90 9A              	        RETF 
                            			.ENDP
                            	
                            	_SP_Export: .PROC
000001BE 88 DA              	        PUSH bp,bp TO [sp] 
000001BF 41 00              	        sp = sp + 1 
000001C0 90 D4              			PUSH r1,r2 TO [sp] 
000001C1 03 92              			r1 = [bp+3]  						// Port Number
000001C2 04 94              			r2 = [bp+4]  						// Value
000001C3 C1 D4              			[r1] = r2  
000001C4 90 90              			POP r1,r2 FROM [sp] 
000001C5 88 98              	        POP bp,bp FROM [sp] 
000001C6 90 9A              	        RETF         
                            	 		.ENDP
                            	
                            	
                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from resource(ROM area)
                            	// 	int SP_GetResource(int Addr, int Page)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
000001C7 88 DA              			push bp to [sp] 
000001C8 08 0B 01 00        			bp = sp + 1 
                            			
000001CA 03 92              			r1 = [bp+3] 					// Address
000001CB 04 94              			r2 = [bp+4] 					// Page
                            			
000001CC 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
000001CD 5A 95              			r2 = r2 lsl 4 
000001CE 4A 95              			r2 = r2 lsl 2 
                            			
000001CF 7F BC              			sr &= 0x03f 					// Change Page
000001D0 06 A5              	        r2 |=sr 						//
000001D1 02 9D              	        sr = r2 						//
                            	  
000001D2 E1 92              	  		r1 = D:[r1] 					// Get data
                            	       
000001D3 88 98              			pop	 bp from [sp] 
000001D4 90 9A              			retf 
                            			.ENDP 
                            	//........................................
                            	F_SP_GetResource:
000001D5 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
000001D6 5A 95              			r2 = r2 lsl 4 
000001D7 4A 95              			r2 = r2 lsl 2 
                            			
000001D8 7F BC              			sr &= 0x03f 					// Change Page
000001D9 06 A5              	        r2 |=sr 						//
000001DA 02 9D              	        sr = r2 						//
                            	  
000001DB E1 92              	  		r1 = D:[r1] 					// Get data
000001DC 90 9A              		  	retf
                            	

00000000 09 93 01 00        			r1 = R_Queue
00000002 40 94              			r2 = 0 
                            	L_ClearQueueLoop?:		
00000003 D1 D4              			[r1++] = r2
00000004 09 43 91 00        			cmp	r1, R_Queue+C_QueueSize

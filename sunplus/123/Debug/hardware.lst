0000000D 12 95 91 00        			r2 = [R_ReadIndex]
0000000F 12 45 92 00        			cmp r2,[R_WriteIndex]
00000011 0D 5E              			je	L_RQ_QueueEmpty
                            	
00000012 0A 05 01 00        			r2 += R_Queue				// get queue data address
00000014 C2 92              			r1 = [r2]
                            			
00000015 12 95 91 00        			r2 = [R_ReadIndex]
00000017 41 04              			r2 += 1
00000018 0A 45 90 00        			cmp	r2, C_QueueSize
0000001A 01 4E              			jne	L_RQ_NotQueueBottom
0000001B 40 94              			r2 = 0
                            	L_RQ_NotQueueBottom:	
0000001C 1A D5 91 00        			[R_ReadIndex] = r2		
                            			//r2 = 0x0000 						// get queue data
0000001E 90 9A              			retf 

                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
00000125 90 D4              			push r1,r2 to [sp] 
00000126 11 93 17 70        	        r1=[P_DAC1] 
00000128 09 B3 C0 FF        	        r1 &= ~0x003f 
0000012A 09 43 00 80        	        cmp     r1,0x8000
0000012C 0E 0E              	        jb     	L_RU_NormalUp
0000012D 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
0000012E 40 F0 91 01        	        call    F_Delay         
00000130 41 94              	        r2 = 0x0001 
00000131 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
00000133 09 23 40 00        	        r1 -= 0x40 
00000135 19 D3 17 70        	        [P_DAC1] = r1 
00000137 09 43 00 80        	        cmp     r1,0x8000 
00000139 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
0000013A 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
0000013B 40 F0 91 01        	        call    F_Delay 
0000013D 41 94              	        r2 = 0x0001 
0000013E 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
00000140 09 03 40 00        	        r1 += 0x40 
00000142 19 D3 17 70        	        [P_DAC1] = r1 
00000144 09 43 00 80        	        cmp     r1, 0x8000 
00000146 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
00000147 90 90              			pop     r1,r2 from [sp] 
00000148 90 9A              	  		retf 
                            	    	.ENDP
                            	    

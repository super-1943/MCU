                            	////////////////////////////////////////////////////////////////
                            	// Function: I/O Port A configuration
                            	// 	void SP_Inti_IOA(int Dir, int Data, int Attrib)
                            	////////////////////////////////////////////////////////////////
                            	_SP_Init_IOA: .PROC
00000197 88 DA              			PUSH bp TO [sp] 
00000198 08 0B 01 00        	        bp = sp + 1 
0000019A 88 D2              			PUSH r1 TO [sp] 
0000019B 03 92              			r1 = [bp+3] 						// Port direction
0000019C 19 D3 02 70        			[P_IOA_Dir] = r1 
0000019E 04 92              			r1 = [bp+4] 
0000019F 19 D3 00 70        			[P_IOA_Data] = r1 
000001A1 05 92              			r1 = [bp+5] 
000001A2 19 D3 03 70        			[P_IOA_Attrib] = r1 
                            			
000001A4 88 90              			POP r1 FROM [sp] 
000001A5 88 98              	        POP bp FROM [sp] 
000001A6 90 9A              	        RETF 
                            			.ENDP
                            	

                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
0000B4B9 90 D4              			push r1,r2 to [sp] 
0000B4BA 11 93 17 70        	        r1=[P_DAC1] 
0000B4BC 09 B3 C0 FF        	        r1 &= ~0x003f 
0000B4BE 09 43 00 80        	        cmp     r1,0x8000
0000B4C0 0E 0E              	        jb     	L_RU_NormalUp
0000B4C1 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
0000B4C2 40 F0 25 B5        	        call    F_Delay         
0000B4C4 41 94              	        r2 = 0x0001 
0000B4C5 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000B4C7 09 23 40 00        	        r1 -= 0x40 
0000B4C9 19 D3 17 70        	        [P_DAC1] = r1 
0000B4CB 09 43 00 80        	        cmp     r1,0x8000 
0000B4CD 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
0000B4CE 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
0000B4CF 40 F0 25 B5        	        call    F_Delay 
0000B4D1 41 94              	        r2 = 0x0001 
0000B4D2 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000B4D4 09 03 40 00        	        r1 += 0x40 
0000B4D6 19 D3 17 70        	        [P_DAC1] = r1 
0000B4D8 09 43 00 80        	        cmp     r1, 0x8000 
0000B4DA 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
0000B4DB 90 90              			pop     r1,r2 from [sp] 
0000B4DC 90 9A              	  		retf 
                            	    	.ENDP
                            	    

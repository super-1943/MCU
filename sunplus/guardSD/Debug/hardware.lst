                            	F_SP_TestQueue_DVR:
                            	F_SP_TestQueue:
                            			//... Test Queue Empty ...
0000B3CB 11 93 EE 04        			R1 = [R_ReadIndex] 
0000B3CD 11 43 EF 04        			cmp R1,[R_WriteIndex] 
0000B3CF 12 5E              			je	L_TQ_QueueEmpty 
                            	
                            			//... Test Queue Full ...
0000B3D0 11 93 EE 04        			R1 = [R_ReadIndex] 				// For N Queue Full: 1.R=0 and W=N-1 2. R<>0 and W=R-1 
0000B3D2 05 4E              			jnz	L_TQ_JudgeCond2 
0000B3D3 11 93 EF 04        			R1 = [R_WriteIndex] 
0000B3D5 09 43 63 00        			cmp	R1, C_QueueSize-1 			// Cond1
0000B3D7 08 5E              			je	L_TQ_QueueFull 			
                            	L_TQ_JudgeCond2:		
0000B3D8 11 93 EE 04        			R1 = [R_ReadIndex] 
0000B3DA 41 22              			R1 -=1 
0000B3DB 11 43 EF 04        			cmp R1,[R_WriteIndex] 
0000B3DD 02 5E              			je	L_TQ_QueueFull 	
                            	
0000B3DE 40 92              			r1 = 0 							// not Full, not empty
0000B3DF 90 9A              			retf 
                            	L_TQ_QueueFull:
0000B3E0 41 92              			r1 = 1 							// full
0000B3E1 90 9A              			retf 
                            	L_TQ_QueueEmpty:
0000B3E2 42 92              			r1 = 2 							// empty
0000B3E3 90 9A              			retf 
                            	
                            	
                            	
                            	
                            	

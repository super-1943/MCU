                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from resource(ROM area)
                            	// 	int SP_GetResource(int Addr, int Page)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
0000B55C 88 DA              			push bp to [sp] 
0000B55D 08 0B 01 00        			bp = sp + 1 
                            			
0000B55F 03 92              			r1 = [bp+3] 					// Address
0000B560 04 94              			r2 = [bp+4] 					// Page
                            			
0000B561 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
0000B562 5A 95              			r2 = r2 lsl 4 
0000B563 4A 95              			r2 = r2 lsl 2 
                            			
0000B564 7F BC              			sr &= 0x03f 					// Change Page
0000B565 06 A5              	        r2 |=sr 						//
0000B566 02 9D              	        sr = r2 						//
                            	  
0000B567 E1 92              	  		r1 = D:[r1] 					// Get data
                            	       
0000B568 88 98              			pop	 bp from [sp] 
0000B569 90 9A              			retf 
                            			.ENDP 
                            	//........................................
                            	F_SP_GetResource:
0000B56A 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
0000B56B 5A 95              			r2 = r2 lsl 4 
0000B56C 4A 95              			r2 = r2 lsl 2 
                            			
0000B56D 7F BC              			sr &= 0x03f 					// Change Page

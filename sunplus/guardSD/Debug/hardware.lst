                            	////////////////////////////////////////////////////////////////
                            	// Function: I/O Port A configuration
                            	// 	void SP_Inti_IOA(int Dir, int Data, int Attrib)
                            	////////////////////////////////////////////////////////////////
                            	_SP_Init_IOA: .PROC
0000B52B 88 DA              			PUSH BP TO [SP] 
0000B52C 08 0B 01 00        	        BP = SP + 1 
0000B52E 88 D2              			PUSH R1 TO [SP] 
0000B52F 03 92              			R1 = [BP+3] 						// Port direction
0000B530 19 D3 02 70        			[P_IOA_Dir] = R1 
0000B532 04 92              			R1 = [BP+4] 
0000B533 19 D3 00 70        			[P_IOA_Data] = R1 
0000B535 05 92              			R1 = [BP+5] 
0000B536 19 D3 03 70        			[P_IOA_Attrib] = R1 
                            			
0000B538 88 90              			POP R1 FROM [SP] 
0000B539 88 98              	        POP BP FROM [SP] 
0000B53A 90 9A              	        RETF 
                            			.ENDP
                            	

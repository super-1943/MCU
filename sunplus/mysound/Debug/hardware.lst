                            	F_SP_ReadQueue_NIC_S480:
                            	F_SP_ReadQueue_NIC_S240:
                            	F_SP_ReadQueue_NIC_MS01:
                            	F_SP_ReadQueue_NIC_DVR:
00008D0B 12 95 86 03        			r2 = [R_ReadIndex]
00008D0D 12 45 87 03        			cmp r2,[R_WriteIndex]
00008D0F 03 5E              			je	L_RQ_QueueEmpty?
                            	
00008D10 0A 05 F6 02        			r2 += R_Queue				// get queue data index
00008D12 C2 92              			r1 = [r2]
                            	L_RQ_QueueEmpty?:
00008D13 90 9A              			RETF
                            			
                            	///////////////////////////////////////////
                            	// Function: Put a data to Queue
                            	// R1: Input
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	F_SP_WriteQueue_A2000:
                            	F_SP_WriteQueue_S480:
                            	F_SP_WriteQueue_S240:
                            	F_SP_WriteQueue_MS01:
                            	F_SP_WriteQueue_DVR:
                            	F_SP_WriteQueue:
00008D14 12 95 87 03        			r2 = [R_WriteIndex] 			// put data to queue
00008D16 0A 05 F6 02        			r2 += R_Queue
00008D18 C2 D2              			[r2] = r1
                            	
00008D19 12 95 87 03        			r2 = [R_WriteIndex]
00008D1B 41 04              			r2 += 1
00008D1C 0A 45 90 00        			cmp	r2, C_QueueSize
00008D1E 01 4E              			jne	L_WQ_NotQueueBottom
00008D1F 40 94              			r2 = 0
                            	L_WQ_NotQueueBottom:
00008D20 1A D5 87 03        			[R_WriteIndex] = r2
00008D22 90 9A              			RETF
                            	
                            			

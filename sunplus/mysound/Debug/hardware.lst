                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
00008E10 90 D4              			push r1,r2 to [sp] 
00008E11 11 93 17 70        	        r1=[P_DAC1] 
00008E13 09 B3 C0 FF        	        r1 &= ~0x003f 
00008E15 09 43 00 80        	        cmp     r1,0x8000
00008E17 0E 0E              	        jb     	L_RU_NormalUp
00008E18 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
00008E19 40 F0 7C 8E        	        call    F_Delay         
00008E1B 41 94              	        r2 = 0x0001 
00008E1C 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
00008E1E 09 23 40 00        	        r1 -= 0x40 
00008E20 19 D3 17 70        	        [P_DAC1] = r1 
00008E22 09 43 00 80        	        cmp     r1,0x8000 
00008E24 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
00008E25 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
00008E26 40 F0 7C 8E        	        call    F_Delay 
00008E28 41 94              	        r2 = 0x0001 
00008E29 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
00008E2B 09 03 40 00        	        r1 += 0x40 
00008E2D 19 D3 17 70        	        [P_DAC1] = r1 
00008E2F 09 43 00 80        	        cmp     r1, 0x8000 
00008E31 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
00008E32 90 90              			pop     r1,r2 from [sp] 
00008E33 90 9A              	  		retf 
                            	    	.ENDP
                            	    

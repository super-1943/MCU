                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from resource(ROM area)
                            	// 	int SP_GetResource(int Addr, int Page)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
00008EB2 88 DA              			push bp to [sp] 
00008EB3 08 0B 01 00        			bp = sp + 1 
                            			
00008EB5 03 92              			r1 = [bp+3] 					// Address
00008EB6 04 94              			r2 = [bp+4] 					// Page
                            			
00008EB7 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
00008EB8 5A 95              			r2 = r2 lsl 4 
00008EB9 4A 95              			r2 = r2 lsl 2 
                            			
00008EBA 7F BC              			sr &= 0x03f 					// Change Page
00008EBB 06 A5              	        r2 |=sr 						//
00008EBC 02 9D              	        sr = r2 						//
                            	  

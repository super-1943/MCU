                            			//... Test Queue Empty ...
00008D23 11 93 86 03        			r1 = [R_ReadIndex] 
00008D25 11 43 87 03        			cmp r1,[R_WriteIndex] 
00008D27 12 5E              			je	L_TQ_QueueEmpty 
                            	
                            			//... Test Queue Full ...
00008D28 11 93 86 03        			r1 = [R_ReadIndex] 				// For N Queue Full: 1.R=0 and W=N-1 2. R<>0 and W=R-1 
00008D2A 05 4E              			jnz	L_TQ_JudgeCond2 
00008D2B 11 93 87 03        			r1 = [R_WriteIndex] 
00008D2D 09 43 8F 00        			cmp	r1, C_QueueSize-1 			// Cond1
00008D2F 08 5E              			je	L_TQ_QueueFull 			
                            	L_TQ_JudgeCond2:		
00008D30 11 93 86 03        			r1 = [R_ReadIndex] 
00008D32 41 22              			r1 -=1 
00008D33 11 43 87 03        			cmp r1,[R_WriteIndex] 
00008D35 02 5E              			je	L_TQ_QueueFull 	
                            	
00008D36 40 92              			r1 = 0 							// not Full, not empty
00008D37 90 9A              			retf 
                            	L_TQ_QueueFull:
00008D38 41 92              			r1 = 1 							// full
00008D39 90 9A              			retf 
                            	L_TQ_QueueEmpty:

                            	F_SP_TestQueue_MS01:
                            	F_SP_TestQueue_DVR:
                            	F_SP_TestQueue:
                            			//... Test Queue Empty ...
0000B5CA 11 93 EE 04        			R1 = [R_ReadIndex] 
0000B5CC 11 43 EF 04        			cmp R1,[R_WriteIndex] 
0000B5CE 12 5E              			je	L_TQ_QueueEmpty 
                            	
                            			//... Test Queue Full ...
0000B5CF 11 93 EE 04        			R1 = [R_ReadIndex] 				// For N Queue Full: 1.R=0 and W=N-1 2. R<>0 and W=R-1 
0000B5D1 05 4E              			jnz	L_TQ_JudgeCond2 
0000B5D2 11 93 EF 04        			R1 = [R_WriteIndex] 
0000B5D4 09 43 63 00        			cmp	R1, C_QueueSize-1 			// Cond1
0000B5D6 08 5E              			je	L_TQ_QueueFull 			
                            	L_TQ_JudgeCond2:		
0000B5D7 11 93 EE 04        			R1 = [R_ReadIndex] 
0000B5D9 41 22              			R1 -=1 
0000B5DA 11 43 EF 04        			cmp R1,[R_WriteIndex] 
0000B5DC 02 5E              			je	L_TQ_QueueFull 	
                            	
0000B5DD 40 92              			r1 = 0 							// not Full, not empty
0000B5DE 90 9A              			retf 
                            	L_TQ_QueueFull:
0000B5DF 41 92              			r1 = 1 							// full
0000B5E0 90 9A              			retf 
                            	L_TQ_QueueEmpty:
0000B5E1 42 92              			r1 = 2 							// empty
0000B5E2 90 9A              			retf 
                            	
                            	
                            	
                            	
                            	

0000B72E 03 92              			R1 = [BP+3] 						// Port direction
0000B72F 19 D3 02 70        			[P_IOA_Dir] = R1 
0000B731 04 92              			R1 = [BP+4] 
0000B732 19 D3 00 70        			[P_IOA_Data] = R1 
0000B734 05 92              			R1 = [BP+5] 
0000B735 19 D3 03 70        			[P_IOA_Attrib] = R1 
                            			
0000B737 88 90              			POP R1 FROM [SP] 
0000B738 88 98              	        POP BP FROM [SP] 
0000B739 90 9A              	        RETF 
                            			.ENDP
                            	

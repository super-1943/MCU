                            	///////////////////////////////////////////
                            	// Function: Initial Queue
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	_SP_InitQueue:	.PROC
                            	_SP_InitQueue_A2000:
                            	_SP_InitQueue_S480:
                            	_SP_InitQueue_S240:
                            	_SP_InitQueue_MS01:
                            	_SP_InitQueue_DVR:
                            	
                            	F_SP_InitQueue_A2000:
                            	F_SP_InitQueue_S480:
                            	F_SP_InitQueue_S240:
                            	F_SP_InitQueue_MS01:
                            	F_SP_InitQueue_DVR:
                            	F_SP_InitQueue:
0000B592 09 93 8A 04        			R1 = R_Queue
0000B594 40 94              			R2 = 0 
                            	L_ClearQueueLoop?:		
0000B595 D1 D4              			[R1++] = R2
0000B596 09 43 EE 04        			cmp	R1, R_Queue+C_QueueSize
0000B598 44 4E              			jne	L_ClearQueueLoop?
                            				
0000B599 40 92              			R1 = 0
0000B59A 19 D3 EE 04        			[R_ReadIndex] = R1
0000B59C 19 D3 EF 04        			[R_WriteIndex] = R1
                            					
0000B59E 90 9A              			RETF
                            			.ENDP
                            			
                            	///////////////////////////////////////////
                            	// Function: Get a data form Queue
                            	// Output:  R1: Data
                            	//			R2: return value
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	F_SP_ReadQueue_A2000:
                            	F_SP_ReadQueue_S480:
                            	F_SP_ReadQueue_S240:
                            	F_SP_ReadQueue_MS01:
                            	F_SP_ReadQueue_DVR:
                            	F_SP_ReadQueue:
0000B59F 12 95 EE 04        			R2 = [R_ReadIndex]
0000B5A1 12 45 EF 04        			cmp R2,[R_WriteIndex]
0000B5A3 0D 5E              			je	L_RQ_QueueEmpty
                            	
0000B5A4 0A 05 8A 04        			R2 += R_Queue				// get queue data address
0000B5A6 C2 92              			R1 = [R2]
                            			
0000B5A7 12 95 EE 04        			R2 = [R_ReadIndex]
0000B5A9 41 04              			R2 += 1
0000B5AA 0A 45 64 00        			cmp	R2, C_QueueSize
0000B5AC 01 4E              			jne	L_RQ_NotQueueBottom
0000B5AD 40 94              			R2 = 0
                            	L_RQ_NotQueueBottom:	
0000B5AE 1A D5 EE 04        			[R_ReadIndex] = R2		
                            			//r2 = 0x0000 						// get queue data
0000B5B0 90 9A              			retf 
                            	L_RQ_QueueEmpty:
                            			//r2 = 0x8000 						// queue empty
0000B5B1 90 9A              			retf 
                            	
                            	///////////////////////////////////////////
                            	// Function: Get a data from Queue but do 
                            	//			not change queue index
                            	// R1: output
                            	// Destory: R1,R2
                            	///////////////////////////////////////////
                            	F_SP_ReadQueue_NIC:
                            	F_SP_ReadQueue_NIC_A2000:
                            	F_SP_ReadQueue_NIC_S480:
                            	F_SP_ReadQueue_NIC_S240:
                            	F_SP_ReadQueue_NIC_MS01:
                            	F_SP_ReadQueue_NIC_DVR:
0000B5B2 12 95 EE 04        			R2 = [R_ReadIndex]
0000B5B4 12 45 EF 04        			cmp R2,[R_WriteIndex]
0000B5B6 03 5E              			je	L_RQ_QueueEmpty?
                            	
0000B5B7 0A 05 8A 04        			R2 += R_Queue				// get queue data index
0000B5B9 C2 92              			R1 = [R2]
                            	L_RQ_QueueEmpty?:
0000B5BA 90 9A              			RETF
                            			
                            	///////////////////////////////////////////
                            	// Function: Put a data to Queue
                            	// R1: Input
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	F_SP_WriteQueue_A2000:
                            	F_SP_WriteQueue_S480:
                            	F_SP_WriteQueue_S240:
                            	F_SP_WriteQueue_MS01:
                            	F_SP_WriteQueue_DVR:
                            	F_SP_WriteQueue:
0000B5BB 12 95 EF 04        			R2 = [R_WriteIndex] 			// put data to queue
0000B5BD 0A 05 8A 04        			R2 += R_Queue
0000B5BF C2 D2              			[R2] = R1
                            	
0000B5C0 12 95 EF 04        			R2 = [R_WriteIndex]
0000B5C2 41 04              			R2 += 1
0000B5C3 0A 45 64 00        			cmp	R2, C_QueueSize
0000B5C5 01 4E              			jne	L_WQ_NotQueueBottom
0000B5C6 40 94              			R2 = 0
                            	L_WQ_NotQueueBottom:
0000B5C7 1A D5 EF 04        			[R_WriteIndex] = R2
0000B5C9 90 9A              			RETF
                            	
                            			

                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
0000B6B8 90 D4              			push r1,r2 to [sp] 
0000B6B9 11 93 17 70        	        r1=[P_DAC1] 
0000B6BB 09 B3 C0 FF        	        r1 &= ~0x003f 
0000B6BD 09 43 00 80        	        cmp     r1,0x8000
0000B6BF 0E 0E              	        jb     	L_RU_NormalUp
0000B6C0 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
0000B6C1 40 F0 24 B7        	        call    F_Delay         
0000B6C3 41 94              	        r2 = 0x0001 
0000B6C4 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000B6C6 09 23 40 00        	        r1 -= 0x40 
0000B6C8 19 D3 17 70        	        [P_DAC1] = r1 
0000B6CA 09 43 00 80        	        cmp     r1,0x8000 
0000B6CC 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
0000B6CD 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
0000B6CE 40 F0 24 B7        	        call    F_Delay 
0000B6D0 41 94              	        r2 = 0x0001 
0000B6D1 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000B6D3 09 03 40 00        	        r1 += 0x40 
0000B6D5 19 D3 17 70        	        [P_DAC1] = r1 
0000B6D7 09 43 00 80        	        cmp     r1, 0x8000 
0000B6D9 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
0000B6DA 90 90              			pop     r1,r2 from [sp] 
0000B6DB 90 9A              	  		retf 
                            	    	.ENDP
                            	    

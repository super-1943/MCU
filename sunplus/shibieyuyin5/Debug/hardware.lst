                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from resource(ROM area)
                            	// 	int SP_GetResource(int Addr, int Page)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
0000B75B 88 DA              			push bp to [sp] 
0000B75C 08 0B 01 00        			bp = sp + 1 
                            			
0000B75E 03 92              			r1 = [bp+3] 					// Address
0000B75F 04 94              			r2 = [bp+4] 					// Page
                            			
0000B760 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
0000B761 5A 95              			r2 = r2 lsl 4 
0000B762 4A 95              			r2 = r2 lsl 2 
                            			
0000B763 7F BC              			sr &= 0x03f 					// Change Page
0000B764 06 A5              	        r2 |=sr 						//
0000B765 02 9D              	        sr = r2 						//
                            	  
0000B766 E1 92              	  		r1 = D:[r1] 					// Get data
                            	       
0000B767 88 98              			pop	 bp from [sp] 
0000B768 90 9A              			retf 
                            			.ENDP 
                            	//........................................
                            	F_SP_GetResource:
0000B769 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
0000B76A 5A 95              			r2 = r2 lsl 4 
0000B76B 4A 95              			r2 = r2 lsl 2 
                            			
0000B76C 7F BC              			sr &= 0x03f 					// Change Page

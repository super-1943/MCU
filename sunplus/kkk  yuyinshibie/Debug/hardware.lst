                            	F_SP_SACM_A2000_Init_:	
0000AAB6 40 92              			R1=0x0000;                      // 24MHz, Fcpu=Fosc
0000AAB7 19 D3 13 70        	        [P_SystemClock]=R1           	//  Frequency 20MHz
0000AAB9 70 92              	        R1 = 0x0030                     // TimerA CKA=Fosc/2 CKB=1 Tout:off

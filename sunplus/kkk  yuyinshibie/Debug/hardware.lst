                            	F_SP_InitQueue_MS01:
                            	F_SP_InitQueue_DVR:
                            	F_SP_InitQueue:
0000AA65 09 93 43 04        			R1 = R_Queue
0000AA67 40 94              			R2 = 0 
                            	L_ClearQueueLoop?:		
0000AA68 D1 D4              			[R1++] = R2
0000AA69 09 43 A7 04        			cmp	R1, R_Queue+C_QueueSize

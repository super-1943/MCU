                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from port
                            	// 	int SP_Import(unsigned int Port)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_Import: .PROC
0000AC1D 88 DA              	        PUSH BP TO [SP] 
0000AC1E 08 0B 01 00        	        BP = SP + 1 
                            	        
0000AC20 03 92              			R1 = [BP+3]  						// Port Number
0000AC21 C1 92              			R1 = [R1]  
                            					
0000AC22 88 98              	        POP BP FROM [SP] 
0000AC23 90 9A              	        RETF 
                            			.ENDP
                            	
                            	_SP_Export: .PROC
0000AC24 88 DA              	        PUSH BP,BP TO [SP] 
0000AC25 08 0B 01 00        	        BP = SP + 1 
0000AC27 90 D4              			PUSH R1,R2 TO [SP] 
0000AC28 03 92              			R1 = [BP+3]  						// Port Number
0000AC29 04 94              			R2 = [BP+4]  						// Value
0000AC2A C1 D4              			[R1] = R2  
0000AC2B 90 90              			POP R1,R2 FROM [SP] 
0000AC2C 88 98              	        POP BP,BP FROM [SP] 
0000AC2D 90 9A              	        RETF         
                            	 		.ENDP
                            	
                            	
                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from resource(ROM area)
                            	// 	int SP_GetResource(int Addr, int Page)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
0000AC2E 88 DA              			push bp to [sp] 
0000AC2F 08 0B 01 00        			bp = sp + 1 
                            			
0000AC31 03 92              			r1 = [bp+3] 					// Address
0000AC32 04 94              			r2 = [bp+4] 					// Page
                            			
0000AC33 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
0000AC34 5A 95              			r2 = r2 lsl 4 
0000AC35 4A 95              			r2 = r2 lsl 2 
                            			
0000AC36 7F BC              			sr &= 0x03f 					// Change Page
0000AC37 06 A5              	        r2 |=sr 						//
0000AC38 02 9D              	        sr = r2 						//
                            	  
0000AC39 E1 92              	  		r1 = D:[r1] 					// Get data
                            	       
0000AC3A 88 98              			pop	 bp from [sp] 
0000AC3B 90 9A              			retf 
                            			.ENDP 
                            	//........................................
                            	F_SP_GetResource:
0000AC3C 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
0000AC3D 5A 95              			r2 = r2 lsl 4 
0000AC3E 4A 95              			r2 = r2 lsl 2 
                            			
0000AC3F 7F BC              			sr &= 0x03f 					// Change Page
0000AC40 06 A5              	        r2 |=sr 						//
0000AC41 02 9D              	        sr = r2 						//
                            	  
0000AC42 E1 92              	  		r1 = D:[r1] 					// Get data
0000AC43 90 9A              		  	retf
                            	

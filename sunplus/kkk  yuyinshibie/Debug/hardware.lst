                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
0000AB8B 90 D4              			push r1,r2 to [sp] 
0000AB8C 11 93 17 70        	        r1=[P_DAC1] 
0000AB8E 09 B3 C0 FF        	        r1 &= ~0x003f 
0000AB90 09 43 00 80        	        cmp     r1,0x8000
0000AB92 0E 0E              	        jb     	L_RU_NormalUp
0000AB93 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
0000AB94 40 F0 F7 AB        	        call    F_Delay         
0000AB96 41 94              	        r2 = 0x0001 
0000AB97 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000AB99 09 23 40 00        	        r1 -= 0x40 
0000AB9B 19 D3 17 70        	        [P_DAC1] = r1 
0000AB9D 09 43 00 80        	        cmp     r1,0x8000 
0000AB9F 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
0000ABA0 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
0000ABA1 40 F0 F7 AB        	        call    F_Delay 
0000ABA3 41 94              	        r2 = 0x0001 
0000ABA4 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000ABA6 09 03 40 00        	        r1 += 0x40 
0000ABA8 19 D3 17 70        	        [P_DAC1] = r1 
0000ABAA 09 43 00 80        	        cmp     r1, 0x8000 
0000ABAC 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
0000ABAD 90 90              			pop     r1,r2 from [sp] 
0000ABAE 90 9A              	  		retf 
                            	    	.ENDP
                            	    

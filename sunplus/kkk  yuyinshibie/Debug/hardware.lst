0000AA72 12 95 A7 04        			R2 = [R_ReadIndex]
0000AA74 12 45 A8 04        			cmp R2,[R_WriteIndex]
0000AA76 0D 5E              			je	L_RQ_QueueEmpty
                            	
0000AA77 0A 05 43 04        			R2 += R_Queue				// get queue data address
0000AA79 C2 92              			R1 = [R2]
                            			
0000AA7A 12 95 A7 04        			R2 = [R_ReadIndex]

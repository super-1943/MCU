                            	////////////////////////////////////////////////////////////////
                            	// Function: I/O Port A configuration
                            	// 	void SP_Inti_IOA(int Dir, int Data, int Attrib)
                            	////////////////////////////////////////////////////////////////
                            	_SP_Init_IOA: .PROC
0000ABFD 88 DA              			PUSH BP TO [SP] 
0000ABFE 08 0B 01 00        	        BP = SP + 1 
0000AC00 88 D2              			PUSH R1 TO [SP] 
0000AC01 03 92              			R1 = [BP+3] 						// Port direction
0000AC02 19 D3 02 70        			[P_IOA_Dir] = R1 
0000AC04 04 92              			R1 = [BP+4] 
0000AC05 19 D3 00 70        			[P_IOA_Data] = R1 
0000AC07 05 92              			R1 = [BP+5] 
0000AC08 19 D3 03 70        			[P_IOA_Attrib] = R1 
                            			
0000AC0A 88 90              			POP R1 FROM [SP] 
0000AC0B 88 98              	        POP BP FROM [SP] 
0000AC0C 90 9A              	        RETF 
                            			.ENDP
                            	

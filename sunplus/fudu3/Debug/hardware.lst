                            	F_SP_TestQueue:
                            			//... Test Queue Empty ...
000098E6 11 93 A5 06        			r1 = [R_ReadIndex] 
000098E8 11 43 A6 06        			cmp r1,[R_WriteIndex] 
000098EA 12 5E              			je	L_TQ_QueueEmpty 
                            	
                            			//... Test Queue Full ...
000098EB 11 93 A5 06        			r1 = [R_ReadIndex] 				// For N Queue Full: 1.R=0 and W=N-1 2. R<>0 and W=R-1 
000098ED 05 4E              			jnz	L_TQ_JudgeCond2 
000098EE 11 93 A6 06        			r1 = [R_WriteIndex] 
000098F0 09 43 8F 00        			cmp	r1, C_QueueSize-1 			// Cond1
000098F2 08 5E              			je	L_TQ_QueueFull 			
                            	L_TQ_JudgeCond2:		
000098F3 11 93 A5 06        			r1 = [R_ReadIndex] 
000098F5 41 22              			r1 -=1 
000098F6 11 43 A6 06        			cmp r1,[R_WriteIndex] 
000098F8 02 5E              			je	L_TQ_QueueFull 	
                            	
000098F9 40 92              			r1 = 0 							// not Full, not empty
000098FA 90 9A              			retf 
                            	L_TQ_QueueFull:
000098FB 41 92              			r1 = 1 							// full
000098FC 90 9A              			retf 
                            	L_TQ_QueueEmpty:
000098FD 42 92              			r1 = 2 							// empty
000098FE 90 9A              			retf 
                            	
                            	
                            	
                            	
                            	

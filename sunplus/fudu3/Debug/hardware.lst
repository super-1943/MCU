                            	///////////////////////////////////////////
                            	// Function: Initial Queue
                            	// Destory: R1,r2
                            	///////////////////////////////////////////	
                            	_SP_InitQueue:	.PROC
                            	_SP_InitQueue_A2000:
                            	_SP_InitQueue_S480:
                            	_SP_InitQueue_S240:
                            	_SP_InitQueue_MS01:
                            	_SP_InitQueue_DVR:
                            	
                            	F_SP_InitQueue_A2000:
                            	F_SP_InitQueue_S480:
                            	F_SP_InitQueue_S240:
                            	F_SP_InitQueue_MS01:
                            	F_SP_InitQueue_DVR:
                            	F_SP_InitQueue:
000098AE 09 93 15 06        			r1 = R_Queue
000098B0 40 94              			r2 = 0 
                            	L_ClearQueueLoop?:		
000098B1 D1 D4              			[r1++] = r2
000098B2 09 43 A5 06        			cmp	r1, R_Queue+C_QueueSize
000098B4 44 4E              			jne	L_ClearQueueLoop?
                            				
000098B5 40 92              			r1 = 0
000098B6 19 D3 A5 06        			[R_ReadIndex] = r1
000098B8 19 D3 A6 06        			[R_WriteIndex] = r1
                            					
000098BA 90 9A              			RETF
                            			.ENDP
                            			
                            	///////////////////////////////////////////
                            	// Function: Get a data form Queue
                            	// Output:  R1: Data
                            	//			R2: return value
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	F_SP_ReadQueue_A2000:
                            	F_SP_ReadQueue_S480:
                            	F_SP_ReadQueue_S240:
                            	F_SP_ReadQueue_MS01:
                            	F_SP_ReadQueue_DVR:
                            	F_SP_ReadQueue:
000098BB 12 95 A5 06        			r2 = [R_ReadIndex]
000098BD 12 45 A6 06        			cmp r2,[R_WriteIndex]
000098BF 0D 5E              			je	L_RQ_QueueEmpty
                            	
000098C0 0A 05 15 06        			r2 += R_Queue				// get queue data address
000098C2 C2 92              			r1 = [r2]
                            			
000098C3 12 95 A5 06        			r2 = [R_ReadIndex]
000098C5 41 04              			r2 += 1
000098C6 0A 45 90 00        			cmp	r2, C_QueueSize
000098C8 01 4E              			jne	L_RQ_NotQueueBottom
000098C9 40 94              			r2 = 0
                            	L_RQ_NotQueueBottom:	
000098CA 1A D5 A5 06        			[R_ReadIndex] = r2		
                            			//r2 = 0x0000 						// get queue data
000098CC 90 9A              			retf 
                            	L_RQ_QueueEmpty:
                            			//r2 = 0x8000 						// queue empty
000098CD 90 9A              			retf 
                            	

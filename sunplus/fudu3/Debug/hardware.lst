                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
000099D4 90 D4              			push r1,r2 to [sp] 
000099D5 11 93 17 70        	        r1=[P_DAC1] 
000099D7 09 B3 C0 FF        	        r1 &= ~0x003f 
000099D9 09 43 00 80        	        cmp     r1,0x8000
000099DB 0E 0E              	        jb     	L_RU_NormalUp
000099DC 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
000099DD 40 F0 40 9A        	        call    F_Delay         
000099DF 41 94              	        r2 = 0x0001 
000099E0 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
000099E2 09 23 40 00        	        r1 -= 0x40 
000099E4 19 D3 17 70        	        [P_DAC1] = r1 
000099E6 09 43 00 80        	        cmp     r1,0x8000 
000099E8 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
000099E9 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
000099EA 40 F0 40 9A        	        call    F_Delay 
000099EC 41 94              	        r2 = 0x0001 
000099ED 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
000099EF 09 03 40 00        	        r1 += 0x40 
000099F1 19 D3 17 70        	        [P_DAC1] = r1 
000099F3 09 43 00 80        	        cmp     r1, 0x8000 
000099F5 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
000099F6 90 90              			pop     r1,r2 from [sp] 
000099F7 90 9A              	  		retf 
                            	    	.ENDP
                            	    

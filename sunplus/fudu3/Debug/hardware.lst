                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from resource(ROM area)
                            	// 	int SP_GetResource(int Addr, int Page)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
00009A76 88 DA              			push bp to [sp] 
00009A77 08 0B 01 00        			bp = sp + 1 
                            			
00009A79 03 92              			r1 = [bp+3] 					// Address
00009A7A 04 94              			r2 = [bp+4] 					// Page
                            			
00009A7B 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
00009A7C 5A 95              			r2 = r2 lsl 4 
00009A7D 4A 95              			r2 = r2 lsl 2 
                            			
00009A7E 7F BC              			sr &= 0x03f 					// Change Page
00009A7F 06 A5              	        r2 |=sr 						//
00009A80 02 9D              	        sr = r2 						//
                            	  
00009A81 E1 92              	  		r1 = D:[r1] 					// Get data
                            	       
00009A82 88 98              			pop	 bp from [sp] 
00009A83 90 9A              			retf 
                            			.ENDP 
                            	//........................................
                            	F_SP_GetResource:
00009A84 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
00009A85 5A 95              			r2 = r2 lsl 4 
00009A86 4A 95              			r2 = r2 lsl 2 
                            			
00009A87 7F BC              			sr &= 0x03f 					// Change Page
00009A88 06 A5              	        r2 |=sr 						//

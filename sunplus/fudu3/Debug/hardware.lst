                            	F_SP_WriteQueue_S480:
                            	F_SP_WriteQueue_S240:
                            	F_SP_WriteQueue_MS01:
                            	F_SP_WriteQueue_DVR:
                            	F_SP_WriteQueue:
000098D7 12 95 A6 06        			r2 = [R_WriteIndex] 			// put data to queue
000098D9 0A 05 15 06        			r2 += R_Queue
000098DB C2 D2              			[r2] = r1
                            	
000098DC 12 95 A6 06        			r2 = [R_WriteIndex]
000098DE 41 04              			r2 += 1
000098DF 0A 45 90 00        			cmp	r2, C_QueueSize
000098E1 01 4E              			jne	L_WQ_NotQueueBottom
000098E2 40 94              			r2 = 0
                            	L_WQ_NotQueueBottom:
000098E3 1A D5 A6 06        			[R_WriteIndex] = r2
000098E5 90 9A              			RETF
                            	
                            			

                            	F_SP_ReadQueue_NIC_S240:
                            	F_SP_ReadQueue_NIC_MS01:
                            	F_SP_ReadQueue_NIC_DVR:
000098CE 12 95 A5 06        			r2 = [R_ReadIndex]
000098D0 12 45 A6 06        			cmp r2,[R_WriteIndex]
000098D2 03 5E              			je	L_RQ_QueueEmpty?
                            	
000098D3 0A 05 15 06        			r2 += R_Queue				// get queue data index
000098D5 C2 92              			r1 = [r2]
                            	L_RQ_QueueEmpty?:
000098D6 90 9A              			RETF
                            			
                            	///////////////////////////////////////////
                            	// Function: Put a data to Queue
                            	// R1: Input
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	

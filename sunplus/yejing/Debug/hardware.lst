000092F4 12 95 87 03        			r2 = [R_WriteIndex] 			// put data to queue
000092F6 0A 05 F6 02        			r2 += R_Queue
000092F8 C2 D2              			[r2] = r1
                            	
000092F9 12 95 87 03        			r2 = [R_WriteIndex]
000092FB 41 04              			r2 += 1
000092FC 0A 45 90 00        			cmp	r2, C_QueueSize
000092FE 01 4E              			jne	L_WQ_NotQueueBottom
000092FF 40 94              			r2 = 0
                            	L_WQ_NotQueueBottom:
00009300 1A D5 87 03        			[R_WriteIndex] = r2
00009302 90 9A              			RETF
                            	
                            			

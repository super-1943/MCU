                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
000093F0 90 D4              			push r1,r2 to [sp] 
000093F1 11 93 17 70        	        r1=[P_DAC1] 
000093F3 09 B3 C0 FF        	        r1 &= ~0x003f 
000093F5 09 43 00 80        	        cmp     r1,0x8000
000093F7 0E 0E              	        jb     	L_RU_NormalUp
000093F8 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
000093F9 40 F0 5C 94        	        call    F_Delay         
000093FB 41 94              	        r2 = 0x0001 
000093FC 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
000093FE 09 23 40 00        	        r1 -= 0x40 
00009400 19 D3 17 70        	        [P_DAC1] = r1 
00009402 09 43 00 80        	        cmp     r1,0x8000 
00009404 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
00009405 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
00009406 40 F0 5C 94        	        call    F_Delay 
00009408 41 94              	        r2 = 0x0001 
00009409 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000940B 09 03 40 00        	        r1 += 0x40 
0000940D 19 D3 17 70        	        [P_DAC1] = r1 
0000940F 09 43 00 80        	        cmp     r1, 0x8000 
00009411 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
00009412 90 90              			pop     r1,r2 from [sp] 
00009413 90 9A              	  		retf 
                            	    	.ENDP
                            	    

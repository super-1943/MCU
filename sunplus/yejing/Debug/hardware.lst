                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from resource(ROM area)
                            	// 	int SP_GetResource(int Addr, int Page)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
00009492 88 DA              			push bp to [sp] 
00009493 08 0B 01 00        			bp = sp + 1 
                            			
00009495 03 92              			r1 = [bp+3] 					// Address
00009496 04 94              			r2 = [bp+4] 					// Page
                            			
00009497 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
00009498 5A 95              			r2 = r2 lsl 4 
00009499 4A 95              			r2 = r2 lsl 2 
                            			
0000949A 7F BC              			sr &= 0x03f 					// Change Page
0000949B 06 A5              	        r2 |=sr 						//
0000949C 02 9D              	        sr = r2 						//
                            	  
0000949D E1 92              	  		r1 = D:[r1] 					// Get data
                            	       
0000949E 88 98              			pop	 bp from [sp] 
0000949F 90 9A              			retf 
                            			.ENDP 
                            	//........................................
                            	F_SP_GetResource:
000094A0 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
000094A1 5A 95              			r2 = r2 lsl 4 
000094A2 4A 95              			r2 = r2 lsl 2 
                            			
000094A3 7F BC              			sr &= 0x03f 					// Change Page
000094A4 06 A5              	        r2 |=sr 						//
000094A5 02 9D              	        sr = r2 						//
                            	  
000094A6 E1 92              	  		r1 = D:[r1] 					// Get data

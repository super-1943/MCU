                            			//... Test Queue Empty ...
00009303 11 93 86 03        			r1 = [R_ReadIndex] 
00009305 11 43 87 03        			cmp r1,[R_WriteIndex] 
00009307 12 5E              			je	L_TQ_QueueEmpty 
                            	
                            			//... Test Queue Full ...
00009308 11 93 86 03        			r1 = [R_ReadIndex] 				// For N Queue Full: 1.R=0 and W=N-1 2. R<>0 and W=R-1 
0000930A 05 4E              			jnz	L_TQ_JudgeCond2 
0000930B 11 93 87 03        			r1 = [R_WriteIndex] 
0000930D 09 43 8F 00        			cmp	r1, C_QueueSize-1 			// Cond1
0000930F 08 5E              			je	L_TQ_QueueFull 			
                            	L_TQ_JudgeCond2:		
00009310 11 93 86 03        			r1 = [R_ReadIndex] 
00009312 41 22              			r1 -=1 
00009313 11 43 87 03        			cmp r1,[R_WriteIndex] 
00009315 02 5E              			je	L_TQ_QueueFull 	
                            	
00009316 40 92              			r1 = 0 							// not Full, not empty
00009317 90 9A              			retf 
                            	L_TQ_QueueFull:
00009318 41 92              			r1 = 1 							// full
00009319 90 9A              			retf 
                            	L_TQ_QueueEmpty:
0000931A 42 92              			r1 = 2 							// empty
0000931B 90 9A              			retf 
                            	
                            	
                            	
                            	
                            	

                            	F_SP_ReadQueue_NIC_S240:
                            	F_SP_ReadQueue_NIC_MS01:
                            	F_SP_ReadQueue_NIC_DVR:
000092EB 12 95 86 03        			r2 = [R_ReadIndex]
000092ED 12 45 87 03        			cmp r2,[R_WriteIndex]
000092EF 03 5E              			je	L_RQ_QueueEmpty?
                            	
000092F0 0A 05 F6 02        			r2 += R_Queue				// get queue data index
000092F2 C2 92              			r1 = [r2]
                            	L_RQ_QueueEmpty?:
000092F3 90 9A              			RETF
                            			
                            	///////////////////////////////////////////
                            	// Function: Put a data to Queue
                            	// R1: Input
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	

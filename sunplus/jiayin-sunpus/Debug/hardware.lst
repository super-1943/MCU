                            	F_SP_ReadQueue_NIC_S240:
                            	F_SP_ReadQueue_NIC_MS01:
                            	F_SP_ReadQueue_NIC_DVR:
000094E0 12 95 D8 02        			r2 = [R_ReadIndex]
000094E2 12 45 D9 02        			cmp r2,[R_WriteIndex]
000094E4 03 5E              			je	L_RQ_QueueEmpty?
                            	
000094E5 0A 05 48 02        			r2 += R_Queue				// get queue data index
000094E7 C2 92              			r1 = [r2]
                            	L_RQ_QueueEmpty?:
000094E8 90 9A              			RETF
                            			
                            	///////////////////////////////////////////
                            	// Function: Put a data to Queue
                            	// R1: Input
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	

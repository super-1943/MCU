                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
00009688 88 DA              			push bp to [sp] 
00009689 08 0B 01 00        			bp = sp + 1 
                            			
0000968B 03 92              			r1 = [bp+3] 					// Address
0000968C 04 94              			r2 = [bp+4] 					// Page
                            			
0000968D 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
0000968E 5A 95              			r2 = r2 lsl 4 
0000968F 4A 95              			r2 = r2 lsl 2 
                            			
00009690 7F BC              			sr &= 0x03f 					// Change Page
00009691 06 A5              	        r2 |=sr 						//
00009692 02 9D              	        sr = r2 						//
                            	  
00009693 E1 92              	  		r1 = D:[r1] 					// Get data
                            	       
00009694 88 98              			pop	 bp from [sp] 
00009695 90 9A              			retf 
                            			.ENDP 

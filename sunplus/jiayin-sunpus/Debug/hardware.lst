000094E9 12 95 D9 02        			r2 = [R_WriteIndex] 			// put data to queue
000094EB 0A 05 48 02        			r2 += R_Queue
000094ED C2 D2              			[r2] = r1
                            	
000094EE 12 95 D9 02        			r2 = [R_WriteIndex]
000094F0 41 04              			r2 += 1
000094F1 0A 45 90 00        			cmp	r2, C_QueueSize
000094F3 01 4E              			jne	L_WQ_NotQueueBottom
000094F4 40 94              			r2 = 0
                            	L_WQ_NotQueueBottom:
000094F5 1A D5 D9 02        			[R_WriteIndex] = r2
000094F7 90 9A              			RETF
                            	
                            			

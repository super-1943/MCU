                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
000095E6 90 D4              			push r1,r2 to [sp] 
000095E7 11 93 17 70        	        r1=[P_DAC1] 
000095E9 09 B3 C0 FF        	        r1 &= ~0x003f 
000095EB 09 43 00 80        	        cmp     r1,0x8000
000095ED 0E 0E              	        jb     	L_RU_NormalUp
000095EE 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
000095EF 40 F0 52 96        	        call    F_Delay         
000095F1 41 94              	        r2 = 0x0001 
000095F2 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
000095F4 09 23 40 00        	        r1 -= 0x40 
000095F6 19 D3 17 70        	        [P_DAC1] = r1 
000095F8 09 43 00 80        	        cmp     r1,0x8000 
000095FA 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
000095FB 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
000095FC 40 F0 52 96        	        call    F_Delay 
000095FE 41 94              	        r2 = 0x0001 
000095FF 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
00009601 09 03 40 00        	        r1 += 0x40 
00009603 19 D3 17 70        	        [P_DAC1] = r1 
00009605 09 43 00 80        	        cmp     r1, 0x8000 
00009607 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
00009608 90 90              			pop     r1,r2 from [sp] 
00009609 90 9A              	  		retf 
                            	    	.ENDP
                            	    

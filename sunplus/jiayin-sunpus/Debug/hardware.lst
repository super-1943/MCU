000094F8 11 93 D8 02        			r1 = [R_ReadIndex] 
000094FA 11 43 D9 02        			cmp r1,[R_WriteIndex] 
000094FC 12 5E              			je	L_TQ_QueueEmpty 
                            	
                            			//... Test Queue Full ...
000094FD 11 93 D8 02        			r1 = [R_ReadIndex] 				// For N Queue Full: 1.R=0 and W=N-1 2. R<>0 and W=R-1 
000094FF 05 4E              			jnz	L_TQ_JudgeCond2 
00009500 11 93 D9 02        			r1 = [R_WriteIndex] 
00009502 09 43 8F 00        			cmp	r1, C_QueueSize-1 			// Cond1
00009504 08 5E              			je	L_TQ_QueueFull 			
                            	L_TQ_JudgeCond2:		
00009505 11 93 D8 02        			r1 = [R_ReadIndex] 
00009507 41 22              			r1 -=1 
00009508 11 43 D9 02        			cmp r1,[R_WriteIndex] 
0000950A 02 5E              			je	L_TQ_QueueFull 	
                            	
0000950B 40 92              			r1 = 0 							// not Full, not empty
0000950C 90 9A              			retf 
                            	L_TQ_QueueFull:
0000950D 41 92              			r1 = 1 							// full
0000950E 90 9A              			retf 
                            	L_TQ_QueueEmpty:
